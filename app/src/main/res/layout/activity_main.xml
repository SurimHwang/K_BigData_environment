<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".MainActivity">

    <fragment
        android:id="@+id/map"
        android:name="com.naver.maps.map.MapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:layout_x="3dp"
        android:layout_y="1dp"
        tools:layout_editor_absoluteX="-163dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_x="0dp"
        android:layout_y="0dp"
        android:background="@drawable/border"
        android:elevation="5dp"
        android:fadingEdge="horizontal|vertical"
        android:focusable="auto"
        android:focusableInTouchMode="true"
        android:gravity="center_horizontal|bottom"
        android:shadowColor="@color/colorPrimary"
        android:shadowRadius="20"
        android:text="정상"
        android:textColor="#000000"
        android:textSize="12sp"
        app:drawableTint="#C5C5C5"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="107dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_x="0dp"
        android:layout_y="45dp"
        android:background="@drawable/border"
        android:elevation="5dp"
        android:gravity="center_horizontal|bottom"
        android:text="관심"
        android:textColor="#000000"
        android:textSize="12sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="157dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_x="0dp"
        android:layout_y="90dp"
        android:background="@drawable/border"
        android:elevation="5dp"
        android:gravity="center_horizontal|bottom"
        android:text="주의"
        android:textColor="#000000"
        android:textSize="12sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="207dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_x="0dp"
        android:layout_y="135dp"
        android:background="@drawable/border"
        android:elevation="5dp"
        android:gravity="center_horizontal|bottom"
        android:text="경계"
        android:textColor="#000000"
        android:textSize="12sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="257dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_x="0dp"
        android:layout_y="180dp"
        android:background="@drawable/border2"
        android:elevation="5dp"
        android:gravity="center_horizontal|bottom"
        android:text="심각"
        android:textColor="#000000"
        android:textSize="12sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="257dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="23dp"
        android:layout_height="35dp"
        android:layout_x="11dp"
        android:layout_y="0dp"
        android:elevation="50dp"
        app:srcCompat="@drawable/green_pin" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="23dp"
        android:layout_height="35dp"
        android:layout_x="11dp"
        android:layout_y="45dp"
        android:elevation="50dp"
        app:srcCompat="@drawable/yellow_pin" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="23dp"
        android:layout_height="35dp"
        android:layout_x="11dp"
        android:layout_y="90dp"
        android:elevation="50dp"
        app:srcCompat="@drawable/orrange_pin" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="23dp"
        android:layout_height="35dp"
        android:layout_x="11dp"
        android:layout_y="135dp"
        android:elevation="50dp"
        app:srcCompat="@drawable/red_pin" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="20dp"
        android:layout_height="33dp"
        android:layout_x="12dp"
        android:layout_y="180dp"
        android:elevation="50dp"
        app:srcCompat="@drawable/black_pin" />

</AbsoluteLayout>