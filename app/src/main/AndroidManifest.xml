<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.naver_map_api">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_drought"
        android:label="쉽고 빠른 가뭄 정보"
        android:roundIcon="@mipmap/ic_launcher_drought"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HtdActivity"></activity>
        <activity android:name=".SubActivity" />
        <activity android:name=".loadingclass">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="ckgflcvluj" />
    </application>

</manifest>